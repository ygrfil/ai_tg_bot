# Telegram Bot Configuration
BOT_TOKEN=your_telegram_bot_token
ALLOWED_USER_IDS=user_id1,user_id2,user_id3
ADMIN_ID=admin_user_id

# OpenRouter Configuration
OPENROUTER_API=your_openrouter_api_key

# Model Configuration
MAX_TOKENS=4096

# Polling Settings (all optional, defaults shown)
# Long polling timeout in seconds
POLLING_TIMEOUT=10
# Minimum interval between requests
POLLING_INTERVAL=0.5
# Maximum delay between retries when rate limited
POLLING_MAX_DELAY=5.0
# Initial retry delay
POLLING_START_DELAY=1.0
# Multiplier for each retry
POLLING_BACKOFF_FACTOR=1.5
# Random jitter to avoid synchronization
POLLING_JITTER=0.1